
@{
    ViewData["Title"] = "Index";
}




<fieldset class="mb-4 mt-4">
    <legend>BusquedaLaboratorio</legend>
    <form class="row g-3">
      <div class="col-md-6">
        <label for="inputEmail4" class="form-label">Nombre</label>
        <input type="email" class="form-control" name="nombre" id="nombre">
      </div>
      <div class="col-md-6">
        <label for="inputPassword4" class="form-label">Dirección</label>
        <input type="password" class="form-control" name="direccion" id="direccion">
      </div>
      <div class="col-12">
        <label for="inputAddress" class="form-label">Persona Contacto</label>
        <input type="text" class="form-control" name="personaContacto" id="personaContacto" placeholder="Contacto...">
      </div>
    </form>
    <br />
    <div class="row g-3">
        <div class="d-grid gap-2">
            
                <button type="submit" class="btn btn-primary">Buscar</button>
            
      
                <button type="submit" class="btn btn-primary">Limpiar</button>
            
        </div>
    </div>
    
</fieldset>

<div id="div-table">
</div>


<script src="~/js/Generic.js"></script>
<script src="~/js/Laboratorio.js"></script>
